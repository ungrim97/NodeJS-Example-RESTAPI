{
  "name": "TechTest-Messages",
  "version": "0.1.1",
  "description": "Simple Message store",
  "main": "bin/server.js",
  "scripts": {
    "dev": "NODE_ENV=development node bin/server.js",
    "dev:dockerize": "docker-compose -f docker-compose.dev.yml up -d --build",
    "start": "node bin/server.js",
    "test": "NODE_ENV=testing mocha test/unit/*",
    "test:integration": "NODE_ENV=testing nyc mocha test/integration/*",
    "test:dockerize": "docker-compose -f docker-compose.test.yml up --force-recreate -V --build --abort-on-container-exit --exit-code-from testMessageStore",
    "migrate:up": "npx sequelize db:migrate",
    "migrate:down": "npx sequelize db:migrate:undo",
    "migrate:reset": "npx sequelize db:migrate:undo:all",
    "dev:fixtures:populate": "NODE_ENV=development npx ./bin/populate-fixtures.js",
    "dev:jwt:generate": "NODE_ENV=development npx ./bin/generate-jwt.js",
    "coverage": "nyc report",
    "tidy": "prettier --write '{bin,src,test}/**/*.js'"
  },
  "author": "Mike Eve",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.4",
    "config": "^3.0.1",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-cookie": "^1.0.0",
    "koa-ctx-paginate": "0.0.3",
    "koa-error": "^3.2.0",
    "koa-jwt": "^3.5.1",
    "koa-morgan": "^1.0.1",
    "koa-paginate": "0.0.4",
    "koa-router": "^7.4.0",
    "koa-server-timing": "^0.2.2",
    "mysql2": "^1.6.5",
    "sequelize": "^5.2.12",
    "sequelize-cli": "^5.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-eslint": "^5.0.0",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2"
  }
}
