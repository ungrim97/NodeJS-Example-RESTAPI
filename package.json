{
  "name": "NodeJS-Examole-RESTAPI",
  "version": "1.0.0",
  "repository": "https://github.com/ungrim97/NodeJS-Example-RESTAPI",
  "description": "Example RESTAPI",
  "main": "bin/server.js",
  "scripts": {
    "coverage": "nyc report",
    "dev:docker:destroy": "docker-compose -f docker-compose.dev.yml down -v",
    "dev:db:start": "docker-compose -f docker-compose.dev.yml up -d --build devMessageStoreDB",
    "dev:db:stop": "docker-compose -f docker-compose.dev.yml stop devMessageStoreDB",
    "dev:docker:start": "docker-compose -f docker-compose.dev.yml up -d --build",
    "dev:docker:stop": "docker-compose -f docker-compose.dev.yml stop devMessageStore",
    "dev:docs:generate": "npx ./bin/generate-swaggerdocs.js",
    "dev:fixtures:populate": "NODE_ENV=development npx ./bin/populate-fixtures.js",
    "dev:fixtures:populate:dockerize": "docker-compose -f docker-compose.dev.yml run devMessageStore npm run dev:fixtures:populate",
    "dev:jwt:generate": "NODE_ENV=development npx ./bin/generate-jwt.js",
    "dev:start": "NODE_ENV=development node bin/server.js",
    "dev:start:pm2": "pm2-runtime start process-dev.config.js",
    "migrate:down": "npx sequelize db:migrate:undo",
    "migrate:reset": "npx sequelize db:migrate:undo:all",
    "migrate:up": "npx sequelize db:migrate",
    "start": "node bin/server.js",
    "start:pm2": "pm2-runtime start process.config.js",
    "test": "NODE_ENV=testing mocha test/unit/* test/eslint.test.js",
    "test:dockerize": "docker-compose -f docker-compose.test.yml up --force-recreate -V --build --abort-on-container-exit --exit-code-from testMessageStore",
    "test:integration": "NODE_ENV=testing nyc mocha test/integration/*",
    "test:unit": "NODE_ENV=testing nyc mocha test/unit/* test/eslint.test.js",
    "tidy": "prettier --write '{bin,src,test,database,config}/**/*.js' '*.js'"
  },
  "author": "Mike Eve",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.4",
    "config": "^3.0.1",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-cookie": "^1.0.0",
    "koa-ctx-paginate": "0.0.3",
    "koa-error": "^3.2.0",
    "koa-etag": "^3.0.0",
    "koa-jwt": "^3.5.1",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-paginate": "0.0.4",
    "koa-router": "^7.4.0",
    "koa-server-timing": "^0.2.2",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^2.13.2",
    "mysql2": "^1.6.5",
    "sequelize": "^5.3.3",
    "sequelize-cli": "^5.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-eslint": "^5.0.0",
    "nyc": "^13.3.0",
    "pm2": "^3.5.0",
    "prettier": "^1.16.4",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.2.9",
    "yaml": "^1.5.0"
  }
}
