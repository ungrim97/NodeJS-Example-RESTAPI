{
  "name": "TechTest-Messages",
  "version": "0.1.1",
  "description": "Simple Message store",
  "main": "bin/server.js",
  "scripts": {
    "dev": "NODE_ENV=development node bin/server.js",
    "start": "node bin/server.js",
    "test": "NODE_ENV=testing mocha test/unit/*",
    "test:integration": "NODE_ENV=testing nyc mocha test/integration/*",
    "test:dockerize": "docker-compose -f test/docker-compose.yml up --build --abort-on-container-exit --exit-code-from messageStore",
    "coverage": "nyc report",
    "tidy": "prettier --write '{bin,src,test}/**/*.js'"
  },
  "author": "Mike Eve",
  "license": "ISC",
  "dependencies": {
    "config": "^3.0.1",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-cookie": "^1.0.0",
    "koa-error": "^3.2.0",
    "koa-jwt": "^3.5.1",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-server-timing": "^0.2.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-eslint": "^5.0.0",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "sinon": "^7.3.1",
    "supertest": "^4.0.2"
  }
}
